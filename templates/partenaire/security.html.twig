										<div id="kt_account_settings_profile_details" class="collapse show">
											{{ form_start(form, {'attr': {'class': 'form fv-plugins-bootstrap5 fv-plugins-framework'}}) }}

											<div class="row mb-6">



												<div class="row mb-6">

													{{ form_label(form.password.first, "Mot de Passe  :", {'label_attr': {'class':"col-lg-4 col-form-label required fw-bold fs-6"}}) }}

													<div class="col-lg-8 fv-row fv-plugins-icon-container">
														{{ form_widget(form.password.first, {'attr': {'class': 'form-control form-control-lg form-control-solid'}}) }}
													</div>
												</div>

												<div class="row mb-6">

													{{ form_label(form.password.second, "Confirmation Mot de Passe :", {'label_attr': {'class':"col-lg-4 col-form-label required fw-bold fs-6"}}) }}

													<div class="col-lg-8 fv-row fv-plugins-icon-container">
